{"ast":null,"code":"// import TheBreadcrumbs from '@/components/layout/TheBreadcrumbs.vue';\n// import CategoryChildren from '@/components/layout/CategoryChildren.vue';\nexport default {\n  name: 'CategoryPage',\n\n  // components: { TheBreadcrumbs, CategoryChildren },\n  data() {\n    return {\n      isLoading: true,\n      slug: null,\n      title: '',\n      categories: []\n    };\n  },\n\n  created() {\n    this.categories = this.fetchData(this.$route.params.slug);\n  },\n\n  watch: {\n    '$route.params.slug': {\n      handler(slug) {\n        this.fetchData(slug);\n      },\n\n      immediate: true\n    }\n  },\n  methods: {\n    async fetchData(slug) {\n      const category = await this.getCategoryPageBySlug(slug);\n      this.title = category.category.length ? category.category[0].title : '';\n      this.categories = category.categories;\n    },\n\n    getCategoryPageBySlug(slug) {\n      return this.$http({\n        method: 'GET',\n        url: `/d/${slug || ''}`\n      }).then(result => {\n        if (result.data) {\n          return result.data;\n        }\n\n        return this.$store.dispatch('setError', result);\n      }).catch(error => this.$store.dispatch('setError', error));\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAuBA;AACA;AAEA,eAAe;AACbA,MAAI,EAAE,cADO;;AAEb;AACAC,MAAI,GAAG;AACL,WAAO;AACLC,eAAS,EAAE,IADN;AAELC,UAAI,EAAE,IAFD;AAGLC,WAAK,EAAE,EAHF;AAILC,gBAAU,EAAE;AAJP,KAAP;AAMD,GAVY;;AAWbC,SAAO,GAAG;AACR,SAAKD,UAAL,GAAkB,KAAKE,SAAL,CAAe,KAAKC,MAAL,CAAYC,MAAZ,CAAmBN,IAAlC,CAAlB;AACD,GAbY;;AAcbO,OAAK,EAAE;AACL,0BAAsB;AACpBC,aAAO,CAACR,IAAD,EAAO;AACZ,aAAKI,SAAL,CAAeJ,IAAf;AACD,OAHmB;;AAIpBS,eAAS,EAAE;AAJS;AADjB,GAdM;AAsBbC,SAAO,EAAE;AACP,UAAMN,SAAN,CAAgBJ,IAAhB,EAAsB;AACpB,YAAMW,QAAO,GAAI,MAAM,KAAKC,qBAAL,CAA2BZ,IAA3B,CAAvB;AACA,WAAKC,KAAL,GAAaU,QAAQ,CAACA,QAAT,CAAkBE,MAAlB,GAA2BF,QAAQ,CAACA,QAAT,CAAkB,CAAlB,EAAqBV,KAAhD,GAAwD,EAArE;AACA,WAAKC,UAAL,GAAkBS,QAAQ,CAACT,UAA3B;AACD,KALM;;AAMPU,yBAAqB,CAACZ,IAAD,EAAO;AAC1B,aAAO,KAAKc,KAAL,CAAW;AA